local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local MainWindow = Rayfield:CreateWindow({
	Name = "Main",
	LoadingTitle = "Booting up the Penguin",
	LoadingSubtitle = "by Classy#6902",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "Pengu Hub"
	},
        Discord = {
        	Enabled = false,
        	Invite = "T5ww6Xb8hJ", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = true -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = true, -- Set this to true to use our key system
	KeySettings = {
		Title = "Pengu Hub",
		Subtitle = "Key System",
		Note = "Join the discord (discord.gg/T5ww6Xb8hJ)",
		FileName = "PenguKey",
		SaveKey = true,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = "Release"
	}
})
local MainTab = MainWindow:CreateTab("Main", 4483362458) -- Title, Image
local MainSection = Tab:CreateSection("Main")
Rayfield:Notify({
    Title = "Thank you for using Pengu Hub!",
    Content = "If you want to become a part of the community or have any feedback, make sure to join the discord!",
    Duration = 6.5,
    Image = 4483362458,
    Actions = { -- Notification Buttons
        Ignore = {
            Name = "Okay!",
            Callback = function()
                print("The user tapped Okay!")
            end
		},
	},
})
local Button = MainTab:CreateButton({
	Name = "(Insert Something to go here)",
	Callback = function()
		-- The function that takes place when the button is pressed
	end,
})
